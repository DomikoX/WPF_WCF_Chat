<Window x:Class="ChatClient.UserNameInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChatClient"
        mc:Ignorable="d"
        Title="Chat - Login" Height="133.053" Width="287">
    <Grid>
        <Label x:Name="Label" Content="Nickname" Margin="10,12,0,0" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" Width="63"/>
        <TextBox x:Name="NicknameBox" Margin="10,38,10,0" TextWrapping="Wrap" Height="22" VerticalAlignment="Top"/>
        <Button x:Name="ButtonOk" Content="Ok" Margin="0,0,10,10" HorizontalAlignment="Right" Width="75" Height="21" VerticalAlignment="Bottom" Click="Button_Click"/>

    </Grid>
</Window>
